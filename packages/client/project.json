{
  "name": "client",
  "root": "packages/client",
  "sourceRoot": "packages/client/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn dev",
        "cwd": "packages/client"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn build",
        "cwd": "packages/client"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn preview",
        "cwd": "packages/client"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn lint",
        "cwd": "packages/client"
      }
    }
  }
}
